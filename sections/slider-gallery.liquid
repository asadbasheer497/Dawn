<script>
  const thumbnailGalleryInnerList_section_{{ section.id | replace: "-", "" }}_padding = document.querySelectorAll('.section-{{ section.id }}-padding .thumbnail_gallery_inner');
  const mainImageContainer_section_{{ section.id | replace: "-", "" }}_padding = document.querySelector('.section-{{ section.id }}-padding .main_image_container');

  thumbnailGalleryInnerList_section_{{ section.id | replace: "-", "" }}_padding.forEach(function (thumbnailGalleryInner) {
    thumbnailGalleryInner.addEventListener('click', function () {
      const clickedImageSrc = thumbnailGalleryInner.querySelector('img').src;
      const mainImage = mainImageContainer_section_{{ section.id | replace: "-", "" }}_padding.querySelector('.main_image_container_inner img');
      if (mainImage) {
        mainImage.src = clickedImageSrc;
      }
$(mainImageContainer_section_{{ section.id | replace: "-", "" }}_padding).slick("refresh");
    
    thumbnailGalleryInner.closest(".main_light_box").querySelector('.slider-gallery').classList.add('active');
        
        const closeButton_section_{{ section.id | replace: "-", "" }} = document.querySelector('.section-{{ section.id }}-padding .close_button');
        closeButton_section_{{ section.id | replace: "-", "" }}.classList.add("atcive_btn")
    });
  });
  
const closeButton_section_{{ section.id | replace: "-", "" }} = document.querySelector('.section-{{ section.id }}-padding .close_button');

closeButton_section_{{ section.id | replace: "-", "" }}.addEventListener('click', function () {
        document.querySelector('.slider-gallery.active').classList.remove('active');
      closeButton_section_{{ section.id | replace: "-", "" }}.classList.remove("atcive_btn")
});


$('.main_image_container').slick({
  slidesToShow: 1,
  slidesToScroll: 1,
  prevArrow: '.prev',
  nextArrow: '.next',
  fade: true,
  infinite:false,
  asNavFor: '.thumbnail_gallery'
});
$('.thumbnail_gallery').slick({
  slidesToShow: 2,
  slidesToScroll: 1,
  asNavFor: '.main_image_container',
  dots: false,
   arrows: false,
  infinite:false,
  focusOnSelect: true
});

  
</script>

