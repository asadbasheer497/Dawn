<style>

  .main_image_container .slick-next:before, .main_image_container .slick-prev:before {
    font-size: 60px;
}
.thumbnail_gallery .slick-track {
    width: 100% !important;
  transform: initial !important;
}
.tcenter{
  text-align: center
}
    .thumbnail_gallery img{
      width:100%;
      height:100%;
      object-fit:cover;
        aspect-ratio: 1/1;
    }
    .thumbnail_gallery, .main_image_container{
      display:flex;
      flex-wrap: wrap;
    }
    .thumbnail_gallery_inner{
width: 25% !important;
    padding: 12px;
    cursor: pointer;
    aspect-ratio: 1/1;
    }
   .section-{{ section.id }}-padding .slider-gallery {
      position:fixed;
      top:50%;
      left:50%;
      transform:translate(-50%, -50%);
      width: 100%;
      height: 100%;
      z-index: 11;
      background: black;
      overflow-y: auto;
      opacity:0;
      visibility:hidden;
    }
 .section-{{ section.id }}-padding .slider-gallery.active{
     opacity:1;
      visibility:visible;
  }
 .main_image_container_inner {
    width: 80%;
    margin: 0 auto;
       margin-bottom: 30px;
       margin-top: 50px;
}
  .main_light_box{
    position:relative;
  }
  button.close_button {
    background: #BFBFBF;
    color: black;
    padding: 10px;
    border-radius: 100%;
    position: fixed;
    right:2% ;
    transition:all 300ms ease;
    display:flex;  
    margin: 7px;
        border: initial;
    z-index:1000;
    top:2%;
    opacity:0;
}
 .section-{{ section.id }}-padding button.atcive_btn{
    opacity:1;
  }
  button.close_button:hover{
     background: white;
  }
  .close_button svg, .custom-prev-main.slick-slide button svg, custom-next-main button svg {
    width:25px;
    height:25px;
  }
  .nagation-wrapper {
    position: sticky;
    top: 50%;
    z-index: 2;
    transform: translateY(-50%);
    display: flex;
    justify-content: space-between;
    padding: 0 14px;
        height: 0;
}
  .main_image_container_inner img {
    width: 100%;
    height: auto;
}
  button.slick-arrow {
    all: inherit;
}
  button.slick-arrow svg {
    width: 40px;
    height: 40px;
}
  button.slick-arrow.slick-disabled {
    opacity: 0.3;
}
  @media screen and (max-width:768px){
    .main_image_container .slick-next.slick-arrow {
    left: 90%;
}
     .main_image_container .slick-next:before, .main_image_container .slick-prev:before {
    font-size: 40px;
}
     .close_button svg {
    width:15px;
    height:15px;
  }
.thumbnail_gallery_inner {
    padding: 0 3px;
    width: calc(50% - 3px) !important;
    margin-bottom: 6px;
}
    .main_image_container_inner {
    margin-bottom: 0px;
    margin-top: 10px;
}
    button.slick-arrow svg {
    width: 28px;
    height: 28px;
}
      .nagation-wrapper {
    padding: 0 6px;
}
  }

  
